<div class="container">
	<%= form_for [@song] do |f|%>
		<h4><%= f.label :title %>
		<%= f.text_field :title %></h4>
		<h4><%= f.label :album %>
		<%= f.text_field :album %></h4>
		<h4><%= f.label :artist %>
		<%= f.text_field :artist %></h4>

		<p><%= f.submit 'Save' %>
		<%= link_to('Back', movies_path) %></p>
	<% end %>
</div>