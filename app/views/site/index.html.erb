<div class="container">
	<h2>Search Song</h2>
	<%= form_tag("/", method: "get") do %>
	<p class="searchBox">
		<%= text_field_tag :search, params[:search] %>
		<%= submit_tag 'Search', :name => nil %>
	</p>
	<% end %>
	<div class="row">
		<div class="col-md-6">
			<h4>Last added movies.</h4>
			<ul><% @movies.each do |movie| %>
				<li>
					<%= link_to(movie.title, movie_path(movie)) %>
					(<%= movie.year %>)
				</li>
			<% end %>
			</ul>
		</div>
		<div class="col-md-6">
			<h4>Last added songs.</h4>
			<ul><% @songs.each do |song| %>
				<li>
					<%= link_to(song.title, song_path(song)) %>
					(<%= song.artist %>)
				</li>
			<% end %>
			</ul>
		</div>
	</div>
</div>